{
  "name": "substrate-docs",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Parity Technolgoies WebDev Team",
  "private": false,
  "license": "MIT",
  "scripts": {
    "clean": "gatsby clean",
    "dev": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "serve:fresh": "yarn clean; yarn build; yarn serve",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "checklinks:v1": "hyperlink --canonicalroot https://docs.substrate.io -r --skip '200 public/rustdocs' --skip 'fragment-redirect' --skip 'fragment-check' --skip 'preconnect-check https://fonts.gstatic.com' --skip 'external-redirect' public/index.html | tap-spot",
    "checklinks:v2": "linkcheck :9000 --skip-file .github/workflows/lc-skip.txt",
    "checklinks:v2:e": "linkcheck :9000 -e --skip-file .github/workflows/lc-skip.txt",
    "checklinks:v3": "blc --filter-level=3 -rof --requests=10 --exclude='/rustdocs' --exclude='/crates.io' --exclude='/fonts.gstatic.com' --exclude='/www.substrate.io' --exclude='/docs.substrate.io' http://localhost:9000",
    "lint": "npm-run-all lint:**",
    "lint:eslint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "lint:tsc": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.6",
    "@mdx-js/react": "^1.6.6",
    "@tailwindcss/forms": "^0.3.3",
    "gatsby": "^3.7.2",
    "gatsby-omni-font-loader": "^1.3.1",
    "gatsby-plugin-eslint": "^3.0.0",
    "gatsby-plugin-image": "^1.8.0",
    "gatsby-plugin-manifest": "^3.7.1",
    "gatsby-plugin-mdx": "^1.2.25",
    "gatsby-plugin-meta-redirect": "^1.1.1",
    "gatsby-plugin-netlify": "^3.12.0",
    "gatsby-plugin-offline": "^4.7.1",
    "gatsby-plugin-react-helmet": "^4.7.1",
    "gatsby-plugin-sass": "^4.7.1",
    "gatsby-plugin-sharp": "^3.7.1",
    "gatsby-plugin-twitter": "^3.7.1",
    "gatsby-remark-autolink-headers": "^4.8.0",
    "gatsby-remark-images": "^5.4.1",
    "gatsby-source-filesystem": "^3.7.1",
    "gatsby-theme-i18n": "^2.0.0",
    "gatsby-theme-i18n-react-intl": "^2.0.0",
    "gatsby-transformer-sharp": "^3.7.1",
    "graphql-type-json": "^0.3.2",
    "lunr": "^2.3.9",
    "mdx-utils": "^0.2.0",
    "prism-react-renderer": "^1.2.1",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dom-confetti": "^0.2.0",
    "react-helmet": "^6.1.0",
    "react-intl": "^5.20.2",
    "react-markdown": "^6.0.2",
    "remark": "13.0.0",
    "sass": "^1.35.1",
    "strip-markdown": "4.0.0"
  },
  "devDependencies": {
    "@types/lunr": "^2.3.4",
    "@types/mdx-js__react": "^1.5.3",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-helmet": "^6.1.1",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "autoprefixer": "^10.2.6",
    "babel-eslint": "^10.1.0",
    "broken-link-checker": "^0.7.8",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-webpack-plugin": "^2.5.4",
    "gatsby-plugin-postcss": "^4.7.1",
    "hyperlink": "^4.7.0",
    "postcss": "^8.3.4",
    "prettier": "^2.3.1",
    "tailwindcss": "^2.2.7",
    "tap-spot": "^1.1.1",
    "typescript": "^4.3.2"
  }
}
