    "build": "gatsby build",
    "serve": "gatsby serve",
    "serve:fresh": "yarn clean; yarn build; yarn serve",
    "fmt:docs": "prettier --config .prettierrc.js --write \"{!**/*.{js,jsx,ts,tsx,json},**/*.{md,mdx}}\"",
